	<script>
	
	(() => {
	
		const state = {
			
			cfg: null,
			
			attachedTargets: new WeakSet(),
			
			allowNative: false,        // used only if you ever decide to replay a click
			
			blockDuringManage: false,  // shields while truste.eu.clickListener() runs
			
			shadowHost: null,
			
			shadowRoot: null,
			
			lastClickedEl: null,
			
			inited: false
		
		};
	
	
	
	// ---- Modal (Shadow DOM) ----
	function ensureModal(cfg) {
		if (state.shadowHost) return;
	
	const host = document.createElement('div');
	host.style.position = 'fixed';
	host.style.inset = '0';
	host.style.zIndex = String(cfg.zIndex);
	host.style.display = 'none';
	host.setAttribute('data-youtube-shield', 'true');
	document.body.appendChild(host);
	
	const root = host.attachShadow({ mode: 'closed' });
	
	
	
	const style = document.createElement('style');
	
	style.textContent = `.backdrop{position:fixed;inset:0;background:rgba(0,0,0,.5);
	
	    display:flex;align-items:center;justify-content:center;
	
	    font-family: system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial;}
	
	  .card{background:#fff;padding:20px;border-radius:12px;
	
	    box-shadow:0 10px 30px rgba(0,0,0,.25);max-width:380px;width:calc(100% - 32px);text-align:center}
	
	  .msg{margin:0 0 16px 0;color:#111;line-height:1.5;font-size:15px}
	
	  .row{display:flex;gap:10px;justify-content:center}
	
	  button{appearance:none;border:0;cursor:pointer;padding:9px 14px;border-radius:8px;font-size:14px}
	
	  .manage{background:#2189ff;color:#fff}
	
	  .cancel{background:#e5e7eb;color:#111}
	`;
	
	
	
	const wrap = document.createElement('div');
	
	wrap.className = 'backdrop';
	
	wrap.innerHTML = `
	
	  <div class="card" role="dialog" aria-modal="true" aria-label="Consent required">
	
	    <p class="msg"></p>
	
	    <div class="row">
	
	      <button class="manage" type="button"></button>
	
	      <button class="cancel" type="button"></button>
	
	    </div>
	
	  </div>
	
	`;
	
	
	
	root.append(style, wrap);
	
	root.querySelector('.msg').textContent = cfg.message;
	
	root.querySelector('.manage').textContent = cfg.manageText;
	
	root.querySelector('.cancel').textContent = cfg.cancelText;
	
	
	
	// Manage Consent
	
	root.querySelector('.manage').addEventListener('click', () => {
	
	  hideModal();
	
	
	
	  // Shield: block any clicks triggered during clickListener()
	
	  state.blockDuringManage = true;
	
	  try {
	
	    if (window.truste?.eu?.clickListener) {
	
	      window.truste.eu.clickListener();
	
	    } else {
	
	      console.warn('[YouTubeShield] window.truste.eu.clickListener not found.');
	
	    }
	
	  } catch (e) {
	
	    console.error('[YouTubeShield] Error calling truste.eu.clickListener:', e);
	
	  } finally {
	
	    // release on microtask to catch sync cascades
	
	    queueMicrotask(() => { state.blockDuringManage = false; });
	
	  }
	
	
	
	  // DO NOT run original click after managing consent
	
	  state.lastClickedEl = null;
	
	});
	
	
	
	// Cancel
	
	root.querySelector('.cancel').addEventListener('click', () => {
	
	  hideModal();
	
	  state.lastClickedEl = null;
	
	});
	
	
	
	// ESC closes
	
	const onKey = (e) => { if (e.key === 'Escape') { hideModal(); state.lastClickedEl = null; } };
	
	document.addEventListener('keydown', onKey);
	
	
	
	state.shadowHost = host;
	
	state.shadowRoot = root;
	
	}
	
	
	
	function showModal() { if (state.shadowHost) state.shadowHost.style.display = 'block'; }
	
	function hideModal() { if (state.shadowHost) state.shadowHost.style.display = 'none'; }
	
	
	
	// ---- Click interception ----
	
	function interceptFactory(el) {
	
	return function interceptClick(e) {
	
	  if (state.allowNative) return; // reserved if you ever replay
	
	  if (state.blockDuringManage) {
	
	    e.preventDefault(); e.stopImmediatePropagation(); e.stopPropagation();
	
	    return;
	
	  }
	
	
	
	  const granted = !!window.consent_granted_for_youtube;
	
	  if (granted) return; // let normal behavior through
	
	
	
	  e.preventDefault();
	
	  e.stopImmediatePropagation();
	
	  e.stopPropagation();
	
	
	
	  state.lastClickedEl = el;
	
	  showModal();
	
	};
	
	}
	
	
	
	function attachTo(el) {
	
	if (state.attachedTargets.has(el)) return;
	
	el.addEventListener('click', interceptFactory(el), { capture: true });
	
	state.attachedTargets.add(el);
	
	}
	
	
	
	function scan() {
	
	if (Array.isArray(state.cfg.selectors) && state.cfg.selectors.length > 0) {
	
	  state.cfg.selectors.forEach(sel => {
	
	    document.querySelectorAll(sel).forEach(attachTo);
	
	    //console log if found nothing for a selector
	
	    if (document.querySelectorAll(sel).length === 0) {
	
	      console.warn('[YouTubeShield] Selector found no elements:' + sel);  
	
	    }
	
	  });
	
	}
	
	// If applyToDataYouTubeID is true, attach to all elements with data-youtube-id
	
	if (state.cfg.applyToDataYouTubeID) {
	
	  document.querySelectorAll('[data-youtube-id]').forEach(attachTo);
	
	}
	
	}
	
	
	
	// ---- Public API ----
	
	function init(cfg) {
	
	if (!cfg ||
	
	    !cfg.message || !cfg.manageText || !cfg.cancelText || !cfg.zIndex) {
	
	  console.error('[YouTubeShield] Missing required config: {message, manageText, cancelText, zIndex}');
	
	  return;
	
	}
	
	if (state.inited) return;
	
	
	
	state.cfg = cfg;
	
	ensureModal(state.cfg);
	
	scan();
	
	
	
	state.inited = true;
	
	}
	
	
	
	function rescan() { if (state.inited) scan(); }
	
	
	
	function destroy() {
	
	if (state.shadowHost) { state.shadowHost.remove(); state.shadowHost = null; state.shadowRoot = null; }
	
	state.inited = false;
	
	}
	
	
	
	window.YouTubeShield = { init, rescan, destroy };
	
	})();
	
	
	
	window.consent_granted_for_youtube = false;
	
	document.addEventListener('DOMContentLoaded', function() {
	
	YouTubeShield.init({
	
	selectors: [],
	
	applyToDataYouTubeID: true,
	
	message: 'YouTube Video Blocked. You need to accept [Advertising/Targeting] cookies to be able to view this content. Please click Manage Consent to update your cookie preferences.',
	
	manageText: 'Manage Consent',
	
	cancelText: 'Cancel',
	
	zIndex: 2147483000
	
	});
	
	});
	
	</script>
	
	
	
